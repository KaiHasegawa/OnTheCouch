<p id="notice"><%= notice %></p>


<table>
  <tr>
    <td>
      <div class="container">
        <% @items.each do |item| %>
          <div class="item-box">
            <%= link_to item,class: "item-image" do %>
              <%= attachment_image_tag item, :image, :fill, 1000, 1000,fallback: "no_images.png", size:'200x230'%><br>
              <br>
              <%= item.name %>
            <% end %>
          </div>
        <% end %>
      </div>
    </td>
  </tr>
</table>
